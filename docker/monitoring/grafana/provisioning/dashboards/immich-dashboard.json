        {
          "query": "import \"math\"\n\nfrom(bucket: \"immich\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"postgresql\")\n  |> filter(fn: (r) => r[\"_field\"] == \"blks_hit\" or r[\"_field\"] == \"blks_read\")\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> map(fn: (r) => ({\n      _time: r._time,\n      db: r.db,\n      _value: if (r.blks_hit + r.blks_read) > 0 then \n                (float(v: r.blks_hit) / float(v: r.blks_hit + r.blks_read)) * 100.0\n              else \n                0.0\n  }))\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  |> keep(columns: [\"_value\", \"_time\", \"db\"])\n  |> yield(name: \"Hit Rate\")",
          "refId": "A"
        }
      ],
      "title": "Cache Hit Ratio",
      "type": "gauge"
